{
    "name": "Cookie Manager",
    "description": "Automated cookie mangager",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "js": [
          "plugin.js"
        ],
        "matches": [
          "https://wordpress.com/"
        ]
      }
    ],
    "background": {
        "service_worker": "plugin.js"
    },
    "permissions": [
      "storage",
      "cookies",
      "https://*/",
      "http://*/"],
    "action": {
        "default_popup": "plugin.html",
        "default_icon": {
            "16": "/images/favicon16x16.png",
            "32": "/images/favicon32x32.png",
            "48": "/images/favicon64x64.png",
            "128": "/images/favicon128x128.png"
          }
      },
      "icons": {
        "16": "/images/favicon16x16.png",
        "32": "/images/favicon32x32.png",
        "48": "/images/favicon64x64.png",
        "128": "/images/favicon128x128.png"
      }
}